{
  "cli": {
    "version": ">= 12.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_VARIANT": "development",
        "EXPO_RESEND_API_KEY": "EXPO_RESEND_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "env": {
        "APP_VARIANT": "production",
        "EXPO_RESEND_API_KEY": "EXPO_RESEND_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
